{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualNetworkGateways_Contoso_Gateway_name": {
            "defaultValue": "Contoso-Gateway",
            "type": "String"
        },
        "publicIPAddresses_Contoso_Gateway_Pip_externalid": {
            "defaultValue": "/subscriptions/bd91dc26-8119-4bbf-882f-84be1cf33026/resourceGroups/Contoso-RG/providers/Microsoft.Network/publicIPAddresses/Contoso-Gateway-Pip",
            "type": "String"
        },
        "virtualNetworks_Hub_externalid": {
            "defaultValue": "/subscriptions/bd91dc26-8119-4bbf-882f-84be1cf33026/resourceGroups/Contoso-RG/providers/Microsoft.Network/virtualNetworks/Hub",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/virtualNetworkGateways",
            "apiVersion": "2020-08-01",
            "name": "[parameters('virtualNetworkGateways_Contoso_Gateway_name')]",
            "location": "westeurope",
            "properties": {
                "enablePrivateIpAddress": false,
                "ipConfigurations": [
                    {
                        "name": "ipConfig1",
                        "properties": {
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[parameters('publicIPAddresses_Contoso_Gateway_Pip_externalid')]"
                            },
                            "subnet": {
                                "id": "[concat(parameters('virtualNetworks_Hub_externalid'), '/subnets/GatewaySubnet')]"
                            }
                        }
                    }
                ],
                "sku": {
                    "name": "VpnGw1",
                    "tier": "VpnGw1"
                },
                "gatewayType": "Vpn",
                "vpnType": "RouteBased",
                "enableBgp": false,
                "activeActive": false,
                "vpnClientConfiguration": {
                    "vpnClientAddressPool": {
                        "addressPrefixes": [
                            "192.168.0.0/24"
                        ]
                    },
                    "vpnClientProtocols": [
                        "SSTP",
                        "IkeV2"
                    ],
                    "vpnClientRootCertificates": [
                        {
                            "name": "ContosoRoot",
                            "properties": {
                                "publicCertData": ""
                            }
                        }
                    ],
                    "vpnClientRevokedCertificates": [],
                    "radiusServers": [],
                    "vpnClientIpsecPolicies": []
                },
                "bgpSettings": {
                    "asn": 65515,
                    "bgpPeeringAddress": "10.0.255.62",
                    "peerWeight": 0,
                    "bgpPeeringAddresses": [
                        {
                            "ipconfigurationId": "[concat(resourceId('Microsoft.Network/virtualNetworkGateways', parameters('virtualNetworkGateways_Contoso_Gateway_name')), '/ipConfigurations/ipConfig1')]",
                            "customBgpIpAddresses": []
                        }
                    ]
                },
                "vpnGatewayGeneration": "Generation1"
            }
        }
    ]
}